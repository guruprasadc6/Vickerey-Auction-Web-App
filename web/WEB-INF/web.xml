<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>webapp.login</servlet-class>
        
    </servlet>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>webapp.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sell</servlet-name>
        <servlet-class>webapp.sell</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>buy</servlet-name>
        <servlet-class>webapp.buy</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>products</servlet-name>
        <servlet-class>webapp.products</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>winners</servlet-name>
        <servlet-class>webapp.winners</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sell</servlet-name>
        <url-pattern>/sell</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>buy</servlet-name>
        <url-pattern>/buy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>products</servlet-name>
        <url-pattern>/products</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>winners</servlet-name>
        <url-pattern>/winners</url-pattern>
    </servlet-mapping>
</web-app>